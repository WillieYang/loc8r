extends layout

block content
  .row.page-header
    .col-lg-12
      br
      h1.text-warning.italic.text-center= title
  .row
    .col-xs-12.mx-auto.col-lg-12
      .card.card-outline-primary.bg_y
        each item in items
          .col-xs-12.col-lg-12.list-group-item.list-group-item-action.active.bg_y
            h6.list-group-item-heading
              a(href="#{item.link}" class="text-warning")= item.title
              br
              p.times-roman
               a(href="#{item.link}" class="text-warning")= item.link